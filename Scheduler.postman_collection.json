{
	"info": {
		"_postman_id": "d5a9fe8c-74a4-40c6-8e3f-2a812e0cb77b",
		"name": "Scheduler",
		"schema": "https://schema.getpostman.com/json/collection/v2.1.0/collection.json",
		"_exporter_id": "21847123"
	},
	"item": [
		{
			"name": "Get All Jobs",
			"request": {
				"method": "GET",
				"header": [
					{"key": "User-Agent", "value": "postman-client"}
				],
				"url": {
					"raw": "http://127.0.0.1:8000/jobs",
					"protocol": "http",
					"host": ["127","0","0","1"],
					"port": "8000",
					"path": ["jobs"]
				}
			}
		},
		{
			"name": "Create Job",
			"request": {
				"method": "POST",
				"header": [
					{"key": "Content-Type", "value": "application/json"},
					{"key": "User-Agent", "value": "postman-client"}
				],
				"body": {
					"mode": "raw",
					"raw": "{\n  \"name\": \"Job Example\",\n  \"function_name\": \"print_hello\",\n  \"interval_seconds\": 10,\n  \"cron_expression\": null,\n  \"job_metadata\": {\n    \"multiplier\": 5,\n    \"text\": \"Hello from Job Example\"\n  },\n  \"status\": \"active\"\n}"
				},
				"url": {
					"raw": "http://127.0.0.1:8000/jobs",
					"protocol": "http",
					"host": ["127","0","0","1"],
					"port": "8000",
					"path": ["jobs"]
				}
			}
		},
		{
			"name": "Get Job by ID",
			"request": {
				"method": "GET",
				"header": [{"key": "User-Agent", "value": "postman-client"}],
				"url": {
					"raw": "http://127.0.0.1:8000/jobs/{{job_id}}",
					"protocol": "http",
					"host": ["127","0","0","1"],
					"port": "8000",
					"path": ["jobs","{{job_id}}"]
				}
			}
		},
		{
			"name": "Replace Job",
			"request": {
				"method": "PUT",
				"header": [
					{"key": "Content-Type", "value": "application/json"},
					{"key": "User-Agent", "value": "postman-client"}
				],
				"body": {
					"mode": "raw",
					"raw": "{\n  \"name\": \"Job Example Updated\",\n  \"function_name\": \"print_hello\",\n  \"interval_seconds\": 15,\n  \"cron_expression\": null,\n  \"job_metadata\": {\n    \"multiplier\": 10,\n    \"text\": \"Updated Job\"\n  },\n  \"status\": \"paused\"\n}"
				},
				"url": {
					"raw": "http://127.0.0.1:8000/jobs/{{job_id}}",
					"protocol": "http",
					"host": ["127","0","0","1"],
					"port": "8000",
					"path": ["jobs","{{job_id}}"]
				}
			}
		},
		{
			"name": "Update Job Partially",
			"request": {
				"method": "PATCH",
				"header": [
					{"key": "Content-Type", "value": "application/json"},
					{"key": "User-Agent", "value": "postman-client"}
				],
				"body": {
					"mode": "raw",
					"raw": "{\n  \"interval_seconds\": 20,\n  \"status\": \"active\"\n}"
				},
				"url": {
					"raw": "http://127.0.0.1:8000/jobs/{{job_id}}",
					"protocol": "http",
					"host": ["127","0","0","1"],
					"port": "8000",
					"path": ["jobs","{{job_id}}"]
				}
			}
		},
		{
			"name": "Delete Job",
			"request": {
				"method": "DELETE",
				"header": [{"key": "User-Agent", "value": "postman-client"}],
				"url": {
					"raw": "http://127.0.0.1:8000/jobs/{{job_id}}?confirm=true",
					"protocol": "http",
					"host": ["127","0","0","1"],
					"port": "8000",
					"path": ["jobs","{{job_id}}"],
					"query":[{"key":"confirm","value":"true"}]
				}
			}
		},
		{
			"name": "Delete All Jobs",
			"request": {
				"method": "DELETE",
				"header": [{"key": "User-Agent", "value": "postman-client"}],
				"url": {
					"raw": "http://127.0.0.1:8000/jobs?confirm=true",
					"protocol": "http",
					"host": ["127","0","0","1"],
					"port": "8000",
					"path": ["jobs"],
					"query":[{"key":"confirm","value":"true"}]
				}
			}
		}
	]
}
